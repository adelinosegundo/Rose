<p id="notice"><%= notice %></p>

<h1>Listing Denuncia</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Hora</th>
      <th>Endereco</th>
      <th>Descricao</th>
      <th>X</th>
      <th>Y</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denuncia.each do |denuncium| %>
      <tr>
        <td><%= denuncium.data %></td>
        <td><%= denuncium.hora %></td>
        <td><%= denuncium.endereco %></td>
        <td><%= denuncium.descricao %></td>
        <td><%= denuncium.x %></td>
        <td><%= denuncium.y %></td>
        <td><%= link_to 'Show', denuncium %></td>
        <td><%= link_to 'Edit', edit_denuncium_path(denuncium) %></td>
        <td><%= link_to 'Destroy', denuncium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denuncium', new_denuncium_path %>
